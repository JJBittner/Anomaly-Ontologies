###  http://example.org/anomaly-continuant-ontology/ACO_0000040
aco:ACO_0000040 rdf:type owl:Class ;
                rdfs:subClassOf aco:ACO_0000041 ;
                rdfs:label "Python Outlier Detection Software Library"@en ;
                skos:definition "A Software Library that implements functions for identifying data patterns that significantly differ from distributional norms."@en ;
                skos:prefLabel "PyOD Library"@en ;
