aro:ARO_0000003 rdf:type owl:Class .


###  http://example.org/anomaly-process-ontology/APO_0000001
apo:APO_0000001 rdf:type owl:Class ;
                rdfs:subClassOf apo:APO_0000004 ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty apo:APO_0000054 ;
                                  owl:someValuesFrom apo:APO_0000050
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000057> ;
                                  owl:someValuesFrom aro:ARO_0000001
                                ] ;
                rdfs:label "Act of Anomaly Resolution"@en ;
                skos:definition "An Act of Resolution in which an Agent interprets or clarifies Unresolved Anomaly Information to produce Resolved Anomaly Information."@en ;
                skos:prefLabel "Act of Anomaly Resolution"@en .


###  http://example.org/anomaly-process-ontology/APO_0000002
apo:APO_0000002 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000366> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty apo:APO_0000054 ;
                                  owl:someValuesFrom apo:APO_0000050
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000057> ;
                                  owl:someValuesFrom <https://www.commoncoreontologies.org/ont00001017>
                                ] ;
                rdfs:label "Epistemic Enlightenment Event"@en ;
                skos:altLabel "EEE"@en ;
                skos:definition "An Act of Anomaly Resolution in which an Agent achieves referential clarity by identifying the actual entity or cause underlying the Anomaly, thereby eliminating its unresolved status."@en ;
                skos:prefLabel "Epistemic Enlightenment Event"@en .


###  http://example.org/anomaly-process-ontology/APO_0000003
apo:APO_0000003 rdf:type owl:Class ;
                rdfs:subClassOf apo:APO_0000012 ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty apo:APO_0000053 ;
                                  owl:someValuesFrom apo:APO_0000050
                                ] ;
                rdfs:label "Act of Anomaly Detection"@en ;
                skos:definition "An Act of Information Processing in which an Agent analyzes input data to identify the presence of one or more Anomalies."@en ;
                skos:prefLabel "Anomaly Detection"@en .


###  http://example.org/anomaly-process-ontology/APO_0000004
apo:APO_0000004 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000366> ;
                rdfs:label "Act of Resolution"@en ;
                skos:definition "An Act of Information Processing in which an Agent clarifies, revises, or replaces an Information Content Entity that lacks semantic closure in order to achieve referential stability."@en ;
                skos:prefLabel "Resolution"@en .


###  http://example.org/anomaly-process-ontology/APO_0000005
apo:APO_0000005 rdf:type owl:Class ;
                rdfs:subClassOf apo:APO_0000004 ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty apo:APO_0000054 ;
                                  owl:someValuesFrom apo:APO_0000050
                                ] ;
                rdfs:label "Invalid Resolution Event"@en ;
                skos:definition "An Act of Resolution that replaces an Unresolved Anomaly Information Content Entity without achieving referential stability."@en ;
                skos:prefLabel "Invalid Resolution Event"@en .


###  http://example.org/anomaly-process-ontology/APO_0000006
apo:APO_0000006 rdf:type owl:Class ;
                rdfs:subClassOf apo:APO_0000001 ;
                rdfs:label "Act of Pseudo-closure"@en ;
                skos:definition "An Act of Anomaly Resolution in which an Anomaly is dismissed or collapsed without achieving referential clarity, resulting in a closure that is semantically invalid."@en ;
                skos:prefLabel "Pseudo-closure"@en .


###  http://example.org/anomaly-process-ontology/APO_0000007
apo:APO_0000007 rdf:type owl:Class ;
                rdfs:subClassOf apo:APO_0000006 ;
                rdfs:label "Correlation-Based Resolution"@en ;
                skos:definition "An Act of Pseudo-closure in which the Anomaly is dismissed through a perceived statistical correlation that lacks causal or referential grounding."@en ;
                skos:prefLabel "Correlation-Based Resolution"@en .


###  http://example.org/anomaly-process-ontology/APO_0000008
apo:APO_0000008 rdf:type owl:Class ;
                rdfs:subClassOf apo:APO_0000010 ;
                rdfs:label "Act of Anomaly Validation"@en ;
                skos:definition "An Act of Validation in which an Agent examines or confirms the significance of an Anomaly prior to an Act of Resolution."@en ;
                skos:prefLabel "Act of Anomaly Validation"@en .


###  http://example.org/anomaly-process-ontology/APO_0000009
apo:APO_0000009 rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000015> ;
                rdfs:label "Resolution Workflow"@en ;
                skos:definition "A Process composed of multiple stages including Act of Detection, Act of Validation, and Act of Resolution executed in sequence or in coordination to restore semantic or operational clarity."@en ;
                skos:prefLabel "Resolution Workflow"@en .


###  http://example.org/anomaly-process-ontology/APO_0000010
apo:APO_0000010 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000366> ;
                rdfs:label "Act of Validation"@en ;
                skos:definition "A Planned Act of Information Processing in which an Agent evaluates or confirms the accuracy, validity, or relevance of an Information Content Entity."@en ;
                skos:prefLabel "Validation"@en .


###  http://example.org/anomaly-process-ontology/APO_0000011
apo:APO_0000011 rdf:type owl:Class ;
                rdfs:subClassOf apo:APO_0000009 ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001777> ;
                                  owl:someValuesFrom apo:APO_0000001
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001777> ;
                                  owl:someValuesFrom apo:APO_0000002
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001777> ;
                                  owl:someValuesFrom apo:APO_0000003
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001777> ;
                                  owl:someValuesFrom apo:APO_0000008
                                ] ;
                rdfs:label "Anomaly Resolution Workflow"@en ;
                skos:definition "A Resolution Workflow specifically structured to address the Act of Anomaly Detection, Act of Anomaly Validation, and Act of Anomaly Resolution that leads to an Epistemic Enlightenment Event in order to restore semantic or operational clarity.\""@en ;
                skos:prefLabel "Anomaly Resolution Workflow"@en .


###  http://example.org/anomaly-process-ontology/APO_0000012
apo:APO_0000012 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000037> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty apo:APO_0000053 ;
                                  owl:someValuesFrom apo:APO_0000050
                                ] ;
                rdfs:label "Act of Detection"@en ;
                skos:definition "An Act of Observation in which an Agent uses a sensor, system, or analytic method to determine whether an entity of interest is present or identifiable within a physical, informational, or computational environment."@en ;
                skos:prefLabel "Detection"@en .


###  http://example.org/anomaly-process-ontology/APO_0000050
apo:APO_0000050 rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000202> ;
                rdfs:label "Bounded Temporaility Interval"@en ;
                skos:definition "A temporal interval that represents the continuous span of time during which a temporally bounded entity, such as an anomaly or role or process exists."@en ;
                skos:prefLabel "Bounded Temporality Interval"@en .


