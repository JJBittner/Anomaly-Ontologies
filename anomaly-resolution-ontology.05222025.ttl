<http://example.org/anomaly-resolution-ontology/> rdf:type owl:Ontology ;
                                                   owl:versionIRI <http://example.org/2025-05-22/anomaly-resolution-ontology/> ;
                                                   owl:imports <https://www.commoncoreontologies.org/2024-11-06/ArtifactOntology> ,
                                                               <https://www.commoncoreontologies.org/2024-11-06/EventOntology> ;
                                                   <http://purl.org/dc/elements/1.1/license> "Creative Commons Attribution 4.0 International License (CC-BY 4.0)"@en ;
                                                   <http://purl.org/dc/terms/contributor> "Jeremy Ravenel"@en ,
                                                                                          "Timothy W. Coleman"@en ;
                                                   <http://purl.org/dc/terms/creator> "John H F Bittner II"@en ;
                                                   rdfs:label "Anomaly Resolution Ontology"@en ;
                                                   skos:definition "An ontology for representing anomalies as generically dependent continuants and the processes that resolve them, integrating both continuant and occurrent entities to support the full lifecycle of anomaly detection, interpretation, and resolution within bounded temporality."@en .

