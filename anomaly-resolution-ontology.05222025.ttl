#################################################################
#    Object Properties
#################################################################

###  http://example.org/anomaly-resolution-ontology/ARO_0000045
aro:ARO_0000045 rdf:type owl:ObjectProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000031> ;
                rdfs:range aro:ARO_0000042 ;
                rdfs:label "has epistemic state"@en ;
                skos:definition "x has an Epistemic State y iff x is an instance of generically dependent continuant, y is an instance of Epistemic State, and y represents the interpretive status of x during a Bounded Temporality Interval."@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000047
aro:ARO_0000047 rdf:type owl:ObjectProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain aro:ARO_0000042 ;
                rdfs:range aro:ARO_0000046 ;
                rdfs:label "interpreted within"@en ;
                skos:definition "x interpreted within y iff x ∈ Epistemic State, y ∈ Epistemic Dimension, and x is a generically dependent continuant whose semantic content is understood relative to the interpretive framework defined by y."@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000051
aro:ARO_0000051 rdf:type owl:ObjectProperty ;
                owl:inverseOf aro:ARO_0000052 ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000015> ;
                rdfs:range aro:ARO_0000059 ;
                rdfs:label "has bounded temporality interval"@en ;
                skos:definition "x has bounded temporality interval y iff x is an instance of bfo:process, y is an instance of temporal interval, and y represents the maximal continuous time span during which x occurs, bounded by epistemic, institutional, or ontological constraints."@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000052
aro:ARO_0000052 rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain aro:ARO_0000059 ;
                rdfs:range <http://purl.obolibrary.org/obo/BFO_0000015> ;
                rdfs:label "is bound by"@en ;
                skos:definition "x is bound by y iff x is an instance of temporal interval, y is an instance of bfo:process, and x represents the bounded temporal extent during which y occurs"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000053
aro:ARO_0000053 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf aro:ARO_0000051 ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000015> ;
                rdfs:range aro:ARO_0000059 ;
                rdfs:label "marks start of bounded temporality interval"@en ;
                skos:definition "x marks start of bounded temporality interval y iff x is an instance of bfo:process, y is an instance of temporal interval, and x is the initiating process that causes y to begin."@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000054
aro:ARO_0000054 rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf aro:ARO_0000051 ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000015> ;
                rdfs:range aro:ARO_0000059 ;
                rdfs:label "marks end of bounded temporality interval"@en ;
                skos:definition "x marks end of bounded temporality interval y iff x is an instance of bfo:process, y is an instance of temporal interval, and x is the terminating event or process that causes y to end."@en .


###  http://purl.obolibrary.org/obo/BFO_0000108
<http://purl.obolibrary.org/obo/BFO_0000108> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://example.org/anomaly-resolution-ontology/ARO_0000001
aro:ARO_0000001 rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000031> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty aro:ARO_0000045 ;
                                  owl:someValuesFrom aro:ARO_0000042
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000056> ;
                                  owl:someValuesFrom aro:ARO_0000058
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000108> ;
                                  owl:someValuesFrom aro:ARO_0000059
                                ] ;
                rdfs:label "Anomaly"@en ;
                skos:definition "A Generically Dependent Continuant that inheres in some entity and represents a deviation from an expected norm within a specified context."@en ;
                skos:prefLabel "Anomaly"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000002
aro:ARO_0000002 rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000031> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000108> ;
                                  owl:someValuesFrom aro:ARO_0000059
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001808> ;
                                  owl:someValuesFrom aro:ARO_0000001
                                ] ;
                rdfs:label "Unresolved Anomaly Information"@en ;
                skos:definition "A Generically Dependent Continuant that represents ambiguous or unverified information about an Anomaly."@en ;
                skos:prefLabel "Unresolved Anomaly Information"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000003
aro:ARO_0000003 rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000031> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001808> ;
                                  owl:someValuesFrom aro:ARO_0000001
                                ] ;
                rdfs:label "Resolved Anomaly Information"@en ;
                skos:definition "A Generically Dependent Continuant that represents confirmed or interpreted explanation about an Anomaly."@en ;
                skos:prefLabel "Resolved Anomaly Information"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000004
aro:ARO_0000004 rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000034> ;
                rdfs:label "Outlier Detection Function"@en ;
                skos:definition "A Realizable Entity that is realized in processes which identify anomalies as outliers in data."@en ;
                skos:prefLabel "Outlier Detection Function"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000005
aro:ARO_0000005 rdf:type owl:Class ;
                rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000040> ;
                rdfs:label "ML Detection Model Artifact"@en ;
                skos:definition "A Material Entity that bears a realizable entity enabling outlier detection."@en ;
                skos:prefLabel "ML Detection Model Artifact"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000006
aro:ARO_0000006 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000293> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001808> ;
                                  owl:someValuesFrom aro:ARO_0000057
                                ] ;
                rdfs:label "Detection Score"@en ;
                skos:definition "A Nomial Measurement Information Content Entity representing a numerical value or label generated by a detection model."@en ;
                skos:prefLabel "Detection Score"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000007
aro:ARO_0000007 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000965> ;
                rdfs:label "Context-Aware Hazard Advisory"@en ;
                skos:definition "A Prescriptive Information Content Entity providing semantically meaningful instructions tied to a Resolved Anomaly."@en ;
                skos:prefLabel "Hazard Advisory"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000008
aro:ARO_0000008 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000958> ;
                rdfs:label "Anomaly Category"@en ;
                skos:definition "An Information Content Entity used to classify or group anomalies (e.g., Sensor Fault vs Pattern Shift)."@en ;
                skos:prefLabel "Anomaly Category"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000009
aro:ARO_0000009 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000745> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001808> ;
                                  owl:someValuesFrom aro:ARO_0000001
                                ] ;
                rdfs:label "Anomaly Confidence Score"@en ;
                skos:definition "A Point Estimate Information Content Entity that expresses the degree of certainty assigned to the classification or detection of an Anomaly."@en ;
                skos:prefLabel "Anomaly Confidence Score"@en ;
                skos:scopeNote "This estimate typically ranges from 0 to 1 or is expressed as a percentage, and it reflects the output of a detection model such as a statistical outlier detector, anomaly classifier, or machine learning algorithm. The value represents the system’s confidence that a given observation or event constitutes an anomaly, based on the input data and model configuration. Anomaly Confidence Score is not a direct measurement but a derived indicator used to support interpretability and decision-making in automated or assisted anomaly resolution processes."@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000010
aro:ARO_0000010 rdf:type owl:Class ;
                rdfs:subClassOf aro:ARO_0000001 ;
                rdfs:label "Terrain-Specific Anomaly"@en ;
                skos:definition "A subclass of Anomaly representing deviation types constrained to geospatial terrain features (e.g., Ice Layer, Slush Zone)."@en ;
                skos:prefLabel "Terrain-Specific Anomaly"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000013
aro:ARO_0000013 rdf:type owl:Class ;
                rdfs:subClassOf aro:ARO_0000004 ;
                rdfs:label "Isolation Forest Function"@en ;
                skos:definition "A specific type of Outlier Detection Function realized by Isolation Forest algorithms for anomaly detection."@en ;
                skos:prefLabel "Isolation Forest Function"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000014
aro:ARO_0000014 rdf:type owl:Class ;
                rdfs:subClassOf aro:ARO_0000004 ;
                rdfs:label "Composite Detection Function"@en ;
                skos:definition "A Function that represents ensembles or voting mechanisms (e.g., AOM in PyOD) for anomaly scoring."@en ;
                skos:prefLabel "Composite Detection Function"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000015
aro:ARO_0000015 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000958> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty aro:ARO_0000045 ;
                                  owl:someValuesFrom aro:ARO_0000042
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000108> ;
                                  owl:someValuesFrom aro:ARO_0000059
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000178> ;
                                  owl:someValuesFrom aro:ARO_0000016
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001808> ;
                                  owl:someValuesFrom aro:ARO_0000001
                                ] ;
                rdfs:label "Anomaly Lifecycle"@en ;
                skos:definition "An Information Content Entity that describes the full semantic span of an Anomaly, beginning at its detection, progressing through its unresolved state, and ending with an Epistemic Enlightenment Event that enables the Anomaly Resolution."@en ;
                skos:prefLabel "Anomaly Lifecycle"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000016
aro:ARO_0000016 rdf:type owl:Class ;
                rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000958> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://www.commoncoreontologies.org/ont00001808> ;
                                  owl:someValuesFrom aro:ARO_0000059
                                ] ;
                rdfs:label "Bounded Temporality"@en ;
                skos:definition "A Generically Dependent Continuant representing the temporal limits within which an Anomaly exists, from Anomaly Detection to Anomaly Resolution."@en ;
                skos:prefLabel "Bounded Temporality"@en .


###  http://example.org/anomaly-resolution-ontology/ARO_0000020
